{
  "name": "geocode-with-opencage-maplibre-thunderforest",
  "description": "Geocding example using Opencage with Maplibre and Thunderforest Vector Tiles",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsamaya/geocode-with-opencage-maplibre-thunderforest.git"
  },
  "bugs": {
    "url": "https://github.com/tsamaya/geocode-with-opencage-maplibre-thunderforest/issues"
  },
  "homepage": "https://github.com/tsamaya/geocode-with-opencage-maplibre-thunderforest#readme",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "clean": "rm -rf dist",
    "check-types": "tsc --noEmit",
    "dev": "vite",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json|md|html)\"",
    "lint": "pnpm dlx eslint  .",
    "lint:fix": "pnpm dlx eslint --fix .",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@eslint/js": "9.32.0",
    "eslint": "9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.4",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.39.0",
    "vite": "7.0.6"
  },
  "dependencies": {
    "@maplibre/maplibre-gl-geocoder": "1.9.0",
    "maplibre-gl": "5.6.1",
    "opencage-api-client": "2.0.0"
  },
  "lint-staged": {
    "*.{json,md,html}": [
      "prettier --write"
    ],
    "*.{js,ts}": "eslint --cache --fix"
  }
}
